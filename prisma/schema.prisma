datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  first_name  String
  last_name   String
  phone String
  emailAddresses EmailAddress[]
}

model EmailAddress {
  id Int @id @default(autoincrement())
  email String @unique
  userId Int
  user User @relation(fields: [userId], references: [id])
}